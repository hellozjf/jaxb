<?xml version="1.0" encoding="utf-8"?>
<!-- 职员档案 所有元素的全集,在在增加、编辑、删除、参照时，可以按具体情况来定节点元素，即除了必须的属性外,其他可以不写 但大小写必续和下面的相符-->
<!--roottag:类型 billtype:系统用 docid:唯一编号 receiver:接收方 sender:发送方编码 proc:操作码 codeexchanged:编码是否转换  exportneedexch导出时是否要根据对照表替换-->
<ufinterface roottag="person" billtype="" docid="" receiver="" sender="" proc="" codeexchanged="" exportneedexch="" version="2.0">
  <person id="">
    <code />
    <name />
    <dept_code />
    <prop />
    <creditline />
    <creditdate />
    <creditgrade />
    <!-- <low_discountrate/>  -->
    <!-- <offergrade/>  -->
    <!-- <offerrate/>  -->
  </person>
</ufinterface>
<!--


code	Varchar	8	职员编号


name	Varchar	10	职员名称


dept_code	Varchar	12	所属部门编号


prop	Varchar	10	职员属性





creditline	float	8	可空	信用额度


creditdate	int	4	可空	信用天数


creditgrade	varchar	6	可空	信用等级


low_discountrate	float	8	可空	最低折扣率


offergrade	varchar	20	可空	销售贡献等级


offerrate	float	8	可空	销售贡献率


-->