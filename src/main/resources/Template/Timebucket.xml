TbCode  	char(4)  	时格代号  	可空


Description  	varchar(60)  	说明  		可空


TbDId	  	int 		明细Id  	非空


TbId  		int  		表头Id  	非空


SortSeq  	int  		行号  		非空


TbType  	int  		类别  		可空


Days  		smallint  	日数  		可空


